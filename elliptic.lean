import Mathlib

#time
set_option maxHeartbeats 0 in
example {x₁ x₂ x₃ y₁ y₂ y₃ a₁ a₂ a₃ a₄ a₆ : Rat}
  (h₁ : y₁^2+a₁*x₁*y₁+a₃*y₁=x₁^3+a₂*x₁^2+a₄*x₁+a₆)
  (h₂ : y₂^2+a₁*x₂*y₂+a₃*y₂=x₂^3+a₂*x₂^2+a₄*x₂+a₆)
  (h₃ : y₃^2+a₁*x₃*y₃+a₃*y₃=x₃^3+a₂*x₃^2+a₄*x₃+a₆):
     (((a₁*x₁+a₁*x₂-y₁+a₁*a₂-a₃)*(x₁-x₂)^3+(2*x₁+x₂-a₁^2+a₂)*(x₁-x₂)^2*(y₁-y₂)-2*a₁*(x₁-x₂)*(y₁-y₂)^2-(y₁-y₂)^3-y₃*(x₁-x₂)^3)^2
      +a₁*((a₁*x₁+a₁*x₂-y₁+a₁*a₂-a₃)*(x₁-x₂)^3+(2*x₁+x₂-a₁^2+a₂)*(x₁-x₂)^2*(y₁-y₂)-2*a₁*(x₁-x₂)*(y₁-y₂)^2-(y₁-y₂)^3-y₃*(x₁-x₂)^3)*((-x₁-x₂-a₂)*(x₁-x₂)^2+a₁*(x₁-x₂)*(y₁-y₂)+(y₁-y₂)^2-x₃*(x₁-x₂)^2)*(x₁-x₂)
      -(a₂*(x₁-x₂)^2+((-x₁-x₂-a₂)*(x₁-x₂)^2+a₁*(x₁-x₂)*(y₁-y₂)+(y₁-y₂)^2)+x₃*(x₁-x₂)^2)*((-x₁-x₂-a₂)*(x₁-x₂)^2+a₁*(x₁-x₂)*(y₁-y₂)+(y₁-y₂)^2-x₃*(x₁-x₂)^2)^2)
    *(x₁*(x₂-x₃)^2-((-x₂-x₃-a₂)*(x₂-x₃)^2+a₁*(x₂-x₃)*(y₂-y₃)+(y₂-y₃)^2))^2*(x₂-x₃)^2
  = ((y₁*(x₂-x₃)^3-((a₁*x₂+a₁*x₃-y₂+a₁*a₂-a₃)*(x₂-x₃)^3+(2*x₂+x₃-a₁^2+a₂)*(x₂-x₃)^2*(y₂-y₃)-2*a₁*(x₂-x₃)*(y₂-y₃)^2-(y₂-y₃)^3))^2
     +a₁*(y₁*(x₂-x₃)^3-((a₁*x₂+a₁*x₃-y₂+a₁*a₂-a₃)*(x₂-x₃)^3+(2*x₂+x₃-a₁^2+a₂)*(x₂-x₃)^2*(y₂-y₃)-2*a₁*(x₂-x₃)*(y₂-y₃)^2-(y₂-y₃)^3))*(x₁*(x₂-x₃)^2-((-x₂-x₃-a₂)*(x₂-x₃)^2+a₁*(x₂-x₃)*(y₂-y₃)+(y₂-y₃)^2))*(x₂-x₃)
     -(a₂*(x₂-x₃)^2+x₁*(x₂-x₃)^2+((-x₂-x₃-a₂)*(x₂-x₃)^2+a₁*(x₂-x₃)*(y₂-y₃)+(y₂-y₃)^2))*(x₁*(x₂-x₃)^2-((-x₂-x₃-a₂)*(x₂-x₃)^2+a₁*(x₂-x₃)*(y₂-y₃)+(y₂-y₃)^2))^2)
   *((-x₁-x₂-a₂)*(x₁-x₂)^2+a₁*(x₁-x₂)*(y₁-y₂)+(y₁-y₂)^2-x₃*(x₁-x₂)^2)^2*(x₁-x₂)^2 := by
  polyrith
